    "@twind/sveltekit": "1.0.0-next.14",
    "prettier": "^2.4.1",
    "prettier-plugin-svelte": "^2.4.0",
    "style-vendorizer": "^2.1.1",
    "svelte": "^3.44.0",
    "svelte-check": "^2.2.6",
    "tslib": "^2.3.1",
    "twind": "1.0.0-next.16",
    "typescript": "^4.4.3"
  }
