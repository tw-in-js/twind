{
  "name": "@example/sveltekit",
  "description": "sveltekit with twind example",
  "version": "0.0.1-next.1",
  "private": true,
  "main": "index.js",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "start": "npm run dev",
    "dev": "svelte-kit dev",
