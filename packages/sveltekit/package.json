{
  "name": "@twind/sveltekit",
  "version": "1.0.0-next.28",
  "description": "sveltekit integration for Twind",
  "publishConfig": {
    "tag": "next",
    "access": "public",
    "directory": "dist"
  },
  "keywords": [
    "twind",
    "tailwind",
    "tailwindcss",
    "tw-in-js",
    "tailwind-in-js",
    "svelte",
    "sveltekit"
  ],
  "type": "module",
  "// The 'module', 'unpkg' and 'types' fields are added by distilt": "",
  "main": "src/index.ts",
  "// Each entry is expanded into several bundles (module, script, types, require, node, and default)": "",
  "exports": {
    ".": {
      "script": null,
      "default": "./src/index.ts"
    },
    "./hooks": {
      "node": "./src/hooks.ts",
      "module": "./src/hooks.ts"
    },
    "./package.json": "./package.json"
  },
  "// These are relative from within the dist/ folder": "",
  "sideEffects": false,
  "peerDependencies": {
    "@sveltejs/kit": "^1.0.0-next.234",
    "twind": "1.0.0-next.28",
    "typescript": "^4.4.0"
  },
  "peerDependenciesMeta": {
    "typescript": {
      "optional": true
    }
  },
  "devDependencies": {
    "@sveltejs/kit": "^1.0.0-next.234",
    "svelte": "^3.44.0",
    "twind": "1.0.0-next.28"
  },
  "scripts": {
    "build": "distilt"
  }
}
