{
  "name": "@twind/with-remix",
  "version": "1.0.0-next.38",
  "description": "remix integration for Twind",
  "publishConfig": {
    "tag": "remix",
    "access": "public",
    "directory": "dist"
  },
  "keywords": [
    "twind",
    "tailwind",
    "tailwindcss",
    "tw-in-js",
    "tailwind-in-js",
    "remix",
    "remix-run"
  ],
  "type": "module",
  "// The 'module', 'unpkg' and 'types' fields are added by distilt": "",
  "main": "src/index.ts",
  "// Each entry is expanded into several bundles (module, script, types, require, node, and default)": "",
  "exports": {
    ".": {
      "script": null,
      "default": "./src/index.ts"
    },
    "./server": {
      "node": "./src/server.ts",
      "module": "./src/server.ts"
    },
    "./package.json": "./package.json"
  },
  "// These are relative from within the dist/ folder": "",
  "sideEffects": false,
  "peerDependencies": {
    "remix": "1.x",
    "react": "^16.6.0 || 17.x || 18.x",
    "twind": "1.0.0-next.38",
    "typescript": "^4.4.0"
  },
  "peerDependenciesMeta": {
    "typescript": {
      "optional": true
    }
  },
  "devDependencies": {
    "remix": "^1.1.3",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "twind": "1.0.0-next.38"
  },
  "scripts": {
    "build": "distilt"
  }
}
