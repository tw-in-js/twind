{
  "name": "twind",
  "version": "1.0.0-next.13",
  "description": "tailwindcss compat based on twind",
  "publishConfig": {
    "tag": "next",
    "access": "public",
    "directory": "dist"
  },
  "keywords": [
    "twind",
    "tailwind",
    "tailwindcss",
    "tw-in-js",
    "tailwind-in-js"
  ],
  "homepage": "https://twind.dev",
  "bugs": "https://github.com/tw-in-js/twind/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/tw-in-js/twind.git",
    "directory": "packages/twind"
  },
  "license": "MIT",
  "contributors": [
    "Luke Jackson (lukejacksonn.github.io)",
    "Sascha Tandel (https://github.com/sastan)"
  ],
  "engines": {
    "node": ">=12.4"
  },
  "type": "module",
  "// The 'module', 'unpkg' and 'types' fields are added by distilt": "",
  "main": "src/index.ts",
  "// Each entry is expanded into several bundles (module, script, types, require, node, and default)": "",
  "exports": {
    ".": "./src/index.ts",
    "./cdn": "./src/cdn.ts",
    "./core": "./src/core.ts",
    "./package.json": "./package.json"
  },
  "// These are relative from within the dist/ folder": "",
  "sideEffects": false,
  "size-limit": [
    {
      "name": "twind/cdn",
      "path": "dist/cdn.esnext.js",
      "brotli": true,
      "limit": "14.6kb"
    },
    {
      "name": "twind (full)",
      "path": "dist/twind.esnext.js",
      "brotli": true,
      "limit": "15.3kb"
    },
    {
      "name": "twind/core (full)",
      "path": "dist/core.esnext.js",
      "brotli": true,
      "limit": "5.8kb"
    }
  ],
  "// These are ONLY bundled (eg included) in the global builds": "",
  "bundledDependencies": [
    "@twind/core",
    "@twind/runtime",
    "@twind/preset-autoprefix",
    "@twind/preset-tailwind"
  ],
  "dependencies": {
    "@twind/core": "workspace:1.0.0-next.12",
    "@twind/preset-autoprefix": "workspace:1.0.0-next.12",
    "@twind/preset-tailwind": "workspace:1.0.0-next.12",
    "@twind/runtime": "workspace:1.0.0-next.12"
  },
  "peerDependencies": {
    "typescript": "^4.4.0"
  },
  "peerDependenciesMeta": {
    "typescript": {
      "optional": true
    }
  },
  "scripts": {
    "prepublishOnly": "distilt"
  }
}
