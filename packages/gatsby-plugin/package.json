{
  "name": "gatsby-plugin-twind",
  "version": "1.0.0-next.35",
  "description": "gatsby integration for Twind",
  "publishConfig": {
    "tag": "next",
    "access": "public",
    "directory": "dist"
  },
  "keywords": [
    "twind",
    "tailwind",
    "tailwindcss",
    "tw-in-js",
    "tailwind-in-js",
    "svelte",
    "gatsby",
    "gatsby-plugin"
  ],
  "type": "commonjs",
  "// The 'module', 'unpkg' and 'types' fields are added by distilt": "",
  "// Each entry is expanded into several bundles (module, script, types, require, node, and default)": "",
  "exports": {
    "./gatsby-browser": {
      "script": null,
      "default": "./src/gatsby-browser.ts"
    },
    "./gatsby-node": {
      "node": "./src/gatsby-node.ts"
    },
    "./gatsby-ssr": {
      "node": "./src/gatsby-ssr.ts"
    },
    "./package.json": "./package.json"
  },
  "// These are relative from within the dist/ folder": "",
  "sideEffects": true,
  "peerDependencies": {
    "gatsby": "2.x || 3.x || 4.x",
    "react": "16.x || 17.x || 18.x",
    "react-dom": "16.x || 17.x || 18.x",
    "twind": "1.0.0-next.35",
    "typescript": "^4.4.0"
  },
  "peerDependenciesMeta": {
    "typescript": {
      "optional": true
    }
  },
  "devDependencies": {
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "gatsby": "^4.5.3",
    "react": "^17.0.0",
    "react-dom": "^17.0.0",
    "twind": "1.0.0-next.35"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "babel-eslint"
      ]
    }
  },
  "scripts": {
    "build": "distilt"
  }
}
