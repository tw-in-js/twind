{
  "name": "@twind/gatsby-plugin",
  "version": "1.0.0-next.22",
  "description": "gatsby integration for Twind",
  "publishConfig": {
    "tag": "next",
    "access": "public",
    "directory": "dist"
  },
  "keywords": [
    "twind",
    "tailwind",
    "tailwindcss",
    "tw-in-js",
    "tailwind-in-js",
    "svelte",
    "gatsby",
    "gatsby-plugin"
  ],
  "homepage": "https://twind.dev",
  "bugs": "https://github.com/tw-in-js/twind/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/tw-in-js/twind.git",
    "directory": "packages/gatsby-plugin"
  },
  "license": "MIT",
  "contributors": [
    "Luke Jackson (lukejacksonn.github.io)",
    "Sascha Tandel (https://github.com/sastan)"
  ],
  "engines": {
    "node": ">=12.4"
  },
  "type": "commonjs",
  "// The 'module', 'unpkg' and 'types' fields are added by distilt": "",
  "// Each entry is expanded into several bundles (module, script, types, require, node, and default)": "",
  "exports": {
    "./gatsby-browser": {
      "node": "./src/gatsby-browser.ts"
    },
    "./gatsby-node": {
      "node": "./src/gatsby-node.ts"
    },
    "./gatsby-ssr": {
      "node": "./src/gatsby-ssr.ts"
    },
    "./package.json": "./package.json"
  },
  "// These are relative from within the dist/ folder": "",
  "sideEffects": true,
  "peerDependencies": {
    "gatsby": "2.x || 3.x || 4.x",
    "react": "16.x || 17.x || 18.x",
    "react-dom": "16.x || 17.x || 18.x",
    "twind": "1.0.0-next.22",
    "typescript": "^4.4.0"
  },
  "peerDependenciesMeta": {
    "typescript": {
      "optional": true
    }
  },
  "devDependencies": {
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "gatsby": "^4.0.0-next",
    "react": "^17.0.0",
    "react-dom": "^17.0.0",
    "twind": "1.0.0-next.22"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "babel-eslint"
      ]
    }
  },
  "scripts": {
    "prepublishOnly": "distilt"
  }
}
