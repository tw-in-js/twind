{
  "name": "@twind/preset-tailwind",
  "version": "1.0.0-next.16",
  "description": "tailwindcss preset for twind",
  "keywords": [
    "twind",
    "tailwind",
    "tailwindcss",
    "tw-in-js",
    "tailwind-in-js",
    "twind-preset",
    "preset"
  ],
  "license": "MIT",
  "type": "module",
  "// The 'module', 'unpkg' and 'types' fields are added by distilt": "",
  "main": "src/index.ts",
  "// Each entry is expanded into several bundles (module, script, types, require, node, and default)": "",
  "exports": {
    ".": "./src/index.ts",
    "./colors": "./src/colors.ts",
    "./defaultTheme": "./src/defaultTheme.ts",
    "./preflight": "./src/preflight.ts",
    "./rules": "./src/rules.ts",
    "./variants": "./src/variants.ts",
    "./package.json": "./package.json"
  },
  "// These are relative from within the dist/ folder": "",
  "sideEffects": false,
  "size-limit": [
    {
      "name": "@twind/preset-tailwind",
      "path": "dist/preset-tailwind.esnext.js",
      "brotli": true,
      "limit": "9.1kb",
      "ignore": [
        "twind"
      ]
    },
    {
      "name": "@twind/preset-tailwind/colors",
      "path": "dist/colors.esnext.js",
      "brotli": true,
      "limit": "1kb"
    },
    {
      "name": "@twind/preset-tailwind/defaultTheme",
      "path": "dist/defaultTheme.esnext.js",
      "brotli": true,
      "limit": "3.5kb"
    }
  ],
  "peerDependencies": {
    "twind": "1.0.0-next.16",
    "typescript": "^4.4.0"
  },
  "peerDependenciesMeta": {
    "typescript": {
      "optional": true
    }
  },
  "devDependencies": {
    "twind": "1.0.0-next.16"
  },
  "scripts": {
    "prepublishOnly": "distilt"
  },
  "publishConfig": {
    "tag": "next",
    "access": "public",
    "directory": "dist"
  }
}
