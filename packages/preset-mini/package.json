{
  "name": "@twind/preset-mini",
  "version": "1.0.0-next.6",
  "description": "mini preset for twind",
  "publishConfig": {
    "tag": "next",
    "access": "public",
    "directory": "dist"
  },
  "keywords": [
    "twind",
    "tailwind",
    "tailwindcss",
    "tw-in-js",
    "tailwind-in-js",
    "twind-preset",
    "preset"
  ],
  "homepage": "https://twind.dev",
  "bugs": "https://github.com/tw-in-js/twind/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/tw-in-js/twind.git",
    "directory": "packages/preset-mini"
  },
  "license": "MIT",
  "contributors": [
    "Luke Jackson (lukejacksonn.github.io)",
    "Sascha Tandel (https://github.com/sastan)"
  ],
  "engines": {
    "node": ">=12.4"
  },
  "type": "module",
  "// The 'module', 'unpkg' and 'types' fields are added by distilt": "",
  "main": "src/index.ts",
  "// Each entry is expanded into several bundles (module, script, types, require, node, and default)": "",
  "exports": {
    ".": "./src/index.ts",
    "./package.json": "./package.json"
  },
  "// These are relative from within the dist/ folder": "",
  "sideEffects": false,
  "size-limit": [
    {
      "name": "@twind/preset-mini",
      "path": "dist/preset-mini.esnext.js",
      "brotli": true,
      "limit": "1kb",
      "ignore": [
        "@twind/core"
      ]
    }
  ],
  "peerDependencies": {
    "@twind/core": "1.0.0-next.6",
    "typescript": "^4.4.0"
  },
  "peerDependenciesMeta": {
    "typescript": {
      "optional": true
    }
  },
  "devDependencies": {
    "@twind/core": "workspace:1.0.0-next.6",
    "@twind/preset-tailwind": "workspace:1.0.0-next.6"
  },
  "scripts": {
    "build": "distilt",
    "prepublishOnly": "npm run build"
  }
}
